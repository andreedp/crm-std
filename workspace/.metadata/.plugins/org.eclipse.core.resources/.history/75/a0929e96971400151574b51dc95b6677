define(['services/services'],
  function(services) {
	services.factory('Contact', ['$resource', 
                                 function($resource) {
	return $resource('/CRM-std/contact/:id',{}, {
		'query': {method:'GET',  isArray:true},
	      'update': {method: 'PUT'},
	      'delete': {method:'DELETE', url: '/CRM-std/contact/:id', params: {id: '@id'}},			
	});
}]);
});